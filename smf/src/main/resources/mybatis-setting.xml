<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias type="kr.co.smf.system.user.User" alias="User" />
        <typeAlias type="kr.co.smf.system.agent.Agent" alias="Agent" />
        <typeAlias type="kr.co.smf.system.measurement.Measurement" alias="Measurement" />
        <typeAlias type="kr.co.smf.system.setting.Setting" alias="Setting" />
    </typeAliases>
</configuration>